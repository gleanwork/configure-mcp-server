#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steve.calvert/workspace/glean/configure-mcp-server/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_msw@2.10.5_@types+node@24.3.0_typescript@5.9.2__tsx@4.20.5_yaml@2.8.1/node_modules/vitest/node_modules:/Users/steve.calvert/workspace/glean/configure-mcp-server/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_msw@2.10.5_@types+node@24.3.0_typescript@5.9.2__tsx@4.20.5_yaml@2.8.1/node_modules:/Users/steve.calvert/workspace/glean/configure-mcp-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steve.calvert/workspace/glean/configure-mcp-server/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_msw@2.10.5_@types+node@24.3.0_typescript@5.9.2__tsx@4.20.5_yaml@2.8.1/node_modules/vitest/node_modules:/Users/steve.calvert/workspace/glean/configure-mcp-server/node_modules/.pnpm/vitest@3.2.4_@types+node@24.3.0_msw@2.10.5_@types+node@24.3.0_typescript@5.9.2__tsx@4.20.5_yaml@2.8.1/node_modules:/Users/steve.calvert/workspace/glean/configure-mcp-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
